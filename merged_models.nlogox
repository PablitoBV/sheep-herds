<?xml version="1.0" encoding="utf-8"?>
<model version="NetLogo 7.0.2" snapToGrid="true">
  <code><![CDATA[globals [
  herd-center
  herd-radius
  current-goal
  waypoints
  waypoint-idx

  ;; Metrics
  score-accumulator
  valid-ticks
  tick-50pct
  tick-80pct
  tick-95pct
  tick-100pct
  current-run-seed
  current-pct-in-target
]

breed [sheep a-sheep]
breed [dogs dog]

sheep-own [ 
  vel-x vel-y
  grazing? 
  graze-timer 
]

;; =============================================================================
;; SETUP
;; =============================================================================

to setup
  clear-all
  set current-run-seed new-seed
  random-seed current-run-seed
  
  set herd-center [0 0]
  set current-goal [0 0]
  set waypoints []
  set waypoint-idx 0
  
  set score-accumulator 0
  set valid-ticks 0
  set tick-50pct -1
  set tick-80pct -1
  set tick-95pct -1
  set tick-100pct -1
  set current-pct-in-target 0

  if not is-string? scenario [ set scenario "Open Field" ]

  draw-scenario
  spawn-agents
  reset-ticks
  
  print (word "--- Run: " num-dogs " dogs | " num-sheep " sheep | Dog Speed: " dog-speed " | Seed: " current-run-seed " ---")
end

to spawn-agents 
  create-sheep num-sheep [ ; create sheep and set up their parameters, spawn in random spot
    set shape "sheep"
    set color white
    set size 1.5
    set vel-x 0 
    set vel-y 0
    set grazing? false
    set graze-timer 0
    
    setxy 0 0
    let safe? false
    while [not safe?] [
       setxy (random-xcor * 0.8) (random-ycor * 0.8)
       if [pcolor] of patch-here != black [ set safe? true ]
    ]
    
    set heading random 360
    let init-speed (max-speed * 0.2)
    set vel-x (sin heading * init-speed)
    set vel-y (cos heading * init-speed)
  ]

  create-dogs num-dogs [ ; create sheep and set up their parameters, spawn in random spot
    set shape "wolf"
    set color orange 
    set size 2
    
    setxy 0 0 
    let safe? false
    while [not safe?] [
       setxy (random-xcor * 0.9) (random-ycor * 0.9)
       if [pcolor] of patch-here != black [ set safe? true ]
    ]
  ]
end

;; =============================================================================
;; MAIN LOOP
;; =============================================================================

to go
  if ticks >= 100000 [ stop ] ;saftey stop at 100k ticks

  calc-herd-metrics
  update-scenario-goals

  ask sheep [ sheep-behavior-strombom ]
  
  let sorted-dogs sort dogs
  let total-dogs count dogs
  foreach sorted-dogs [ d ->
    ask d [ dog-behavior (position d sorted-dogs) total-dogs ]
  ]

  update-metrics
  tick
end

;; =============================================================================
;; SHEEP LOGIC (Strombom k-NN)
;; =============================================================================

to sheep-behavior-strombom ; Sheep moving from l√∂ogic from the Strombom paper
  let dt 0.2
  let k k-neighbors 
  
  ;; 1. Fear of dogs vector
  let nearby-dogs dogs in-radius 10
  let is-afraid? false
  let fear-vec-x 0 
  let fear-vec-y 0 

  if any? nearby-dogs [
    ask nearby-dogs [
      let d distance myself
      if d < 10 and d > 0 [ 
        set is-afraid? true
        let diff-x ([xcor] of myself - xcor)
        let diff-y ([ycor] of myself - ycor)
        let repulsion (1 / (d * d)) * fear-of-dogs 
        set fear-vec-x fear-vec-x + (diff-x * repulsion)
        set fear-vec-y fear-vec-y + (diff-y * repulsion)
      ]
    ]
  ]
  if is-afraid? [ set grazing? false ]

  ;; 2. Interaction between sheeps (separation vector, cohesion vector and alignment vector)
  ifelse grazing? [
    set vel-x 0 set vel-y 0
    set graze-timer graze-timer - 1
    if graze-timer <= 0 [ set grazing? false ]
    if count other sheep in-radius 2.0 > 3 [ set grazing? false ]
  ] [
    let neighbors-list (ifelse-value (count other sheep >= k)
      [ sublist sort-on [distance myself] other sheep 0 k ]
      [ sort-on [distance myself] other sheep ])
      
    let neighbor-set turtle-set neighbors-list
    let sep-vec-x 0   let sep-vec-y 0
    let align-vec-x 0 let align-vec-y 0
    let coh-vec-x 0   let coh-vec-y 0
    
    if not empty? neighbors-list [
      foreach neighbors-list [ n ->
        let d distance n
        if (d < separation-distance and d > 0) [
          let away-x (xcor - [xcor] of n)
          let away-y (ycor - [ycor] of n)
          set sep-vec-x sep-vec-x + (away-x / (d * d))
          set sep-vec-y sep-vec-y + (away-y / (d * d))
        ]
      ]
    ]

    if any? neighbor-set [
      set align-vec-x mean [vel-x] of neighbor-set
      set align-vec-y mean [vel-y] of neighbor-set
      set coh-vec-x ((mean [xcor] of neighbor-set) - xcor)
      set coh-vec-y ((mean [ycor] of neighbor-set) - ycor)
    ]
  
    ;; 3. Wall avoidance vector
    let wall-vec-x 0 let wall-vec-y 0
    let scan-dist 2.0
    let ahead-x xcor + vel-x * scan-dist
    let ahead-y ycor + vel-y * scan-dist
    
    if ahead-x > max-pxcor [ set ahead-x max-pxcor ]
    if ahead-x < min-pxcor [ set ahead-x min-pxcor ]
    if ahead-y > max-pycor [ set ahead-y max-pycor ]
    if ahead-y < min-pycor [ set ahead-y min-pycor ]
    
    if [pcolor] of patch ahead-x ahead-y = black [
       let wx ahead-x - xcor
       let wy ahead-y - ycor
       set wall-vec-x (-1 * wx * 50) 
       set wall-vec-y (-1 * wy * 50)
    ]
    
    ;; Combine all relevant vectors
    let ax (sep-vec-x * separation-weight) + (align-vec-x * alignment-weight) + (coh-vec-x * cohesion-weight) + (fear-vec-x * 8.0) + wall-vec-x
    let ay (sep-vec-y * separation-weight) + (align-vec-y * alignment-weight) + (coh-vec-y * cohesion-weight) + (fear-vec-y * 8.0) + wall-vec-y
    
    set vel-x vel-x + (ax * dt)
    set vel-y vel-y + (ay * dt)
    set vel-x vel-x * 0.90 
    set vel-y vel-y * 0.90
    set vel-x vel-x + ((random-float (noise * 2) - noise) * dt)
    set vel-y vel-y + ((random-float (noise * 2) - noise) * dt)
    
    let limit-speed (max-speed / 5.0) 
    let v-mag sqrt (vel-x * vel-x + vel-y * vel-y)
    if v-mag > limit-speed [ 
      set vel-x (vel-x / v-mag) * limit-speed 
      set vel-y (vel-y / v-mag) * limit-speed 
    ]
    
    if v-mag > 0.01 [ set heading atan vel-x vel-y ]
    
    let next-x xcor + (vel-x * dt)
    let next-y ycor + (vel-y * dt)
    
    if next-x > max-pxcor [ set next-x max-pxcor set vel-x vel-x * -0.5 ]
    if next-x < min-pxcor [ set next-x min-pxcor set vel-x vel-x * -0.5 ]
    if next-y > max-pycor [ set next-y max-pycor set vel-y vel-y * -0.5 ]
    if next-y < min-pycor [ set next-y min-pycor set vel-y vel-y * -0.5 ]

    ifelse [pcolor] of patch next-x next-y != black [
      set xcor next-x 
      set ycor next-y
    ] [
      set vel-x 0
      set vel-y 0
    ]
    
    if not is-afraid? [
      if random-float 100 < 5.0 [ 
        set grazing? true 
        set graze-timer 250 + random 50 
      ]
    ]
  ]
end

;; =============================================================================
;; DOG LOGIC
;; =============================================================================

to dog-behavior [ rank total-dogs ]
  let cx item 0 herd-center
  let cy item 1 herd-center
  
  let target-x 0
  let target-y 0
  let is-collecting? false
  
  ;; --- Shared sensing of the straggler, the furthest sheep from the sheep-centroid
  let straggler nobody
  let dist-straggler 0

  ifelse scenario = "Enclosure" [
    set straggler max-one-of sheep with [abs xcor > 9 or abs ycor > 9] [distancexy cx cy]
    if straggler = nobody [ set straggler max-one-of sheep [distancexy cx cy] ]
  ] [
    set straggler max-one-of sheep [distancexy cx cy]
  ]
  if straggler != nobody [ set dist-straggler [distancexy cx cy] of straggler ]


  ;; --- Strategy switching ---

  if dog-strategy = "Surround" [
    set color orange
    
    ; DRIVE LOGIC (push sheep towards goal)
    let focus-x cx
    let focus-y cy
    
    if scenario != "Open Field" [
       let gx item 0 current-goal
       let gy item 1 current-goal
       let dist-to-goal sqrt ((gx - cx)^ 2 + (gy - cy)^ 2)
       
       ; Shift the center of the orbit 3.0 units towards the goal
       if dist-to-goal > 0 [
          set focus-x cx + ((gx - cx) / dist-to-goal) * 3.0
          set focus-y cy + ((gy - cy) / dist-to-goal) * 3.0
       ]
    ]

    ; SURROUND LOGIC (orbit around the sheep in a circle to contain them)
    let max-spread 0
    if any? sheep [ set max-spread max [distancexy cx cy] of sheep ]
    
    ; Orbit dist is max(10, spread + 5)
    let orbit-dist max (list 10 (max-spread + 5))
    
    let angle-step 360 / total-dogs
    let my-angle (rank * angle-step)
    
    set target-x focus-x + (sin my-angle * orbit-dist)
    set target-y focus-y + (cos my-angle * orbit-dist)

    ; If a straggler is > 15 units away, the closest dog breaks formation to get it
    if straggler != nobody and  dist-straggler > (herd-threshold * 0.6)[
       let closest-dog min-one-of dogs [distance straggler]
       if closest-dog = self [
          set is-collecting? true
          set color blue
          
          let sx [xcor] of straggler
          let sy [ycor] of straggler
          let v-len distancexy sx sy
          if v-len = 0 [ set v-len 0.001 ]
          
          ; Target a point behind the straggler relative to the herd center
          let vec-x (sx - cx) / v-len
          let vec-y (sy - cy) / v-len
          set target-x sx + (vec-x * 4)
          set target-y sy + (vec-y * 4)
       ]
    ]
  ]

  if dog-strategy = "Surround + Drive" [ ; Surround logic with 1 dog trying to catch stragglers while others just surround
    ;; 1 dog is in charge of catching stragglers
    ifelse (straggler != nobody and dist-straggler > (herd-threshold * 0.6)) and (rank = 0) [
      set is-collecting? true
      set color blue
      let sx [xcor] of straggler
      let sy [ycor] of straggler
      let v-len distancexy sx sy
      if v-len = 0 [ set v-len 0.001 ]
      let vec-x (sx - cx) / v-len
      let vec-y (sy - cy) / v-len
      set target-x sx + (vec-x * 4)
      set target-y sy + (vec-y * 4)
    ] [
      set color orange
      let focus-x cx
      let focus-y cy
      let gx item 0 current-goal
      let gy item 1 current-goal
      
      if scenario != "Open Field" [
         let dist-to-goal sqrt ((gx - cx)^ 2 + (gy - cy)^ 2)
         if dist-to-goal > 0 [
           set focus-x cx + ((gx - cx) / dist-to-goal) * 3.0
           set focus-y cy + ((gy - cy) / dist-to-goal) * 3.0
         ]
      ]
      
      let max-spread 0
      if any? sheep [ set max-spread max [distancexy cx cy] of sheep ]
      let orbit-dist max (list 10 (max-spread + 5))
      let angle-step 360 / total-dogs
      let my-angle (rank * angle-step)
      set target-x focus-x + (sin my-angle * orbit-dist)
      set target-y focus-y + (cos my-angle * orbit-dist)
    ]
  ]

  if dog-strategy = "Rotate" [ ;; ROTATE logic:  1 dog catches stragglers, while others rotate around the herd
    
    ifelse (straggler != nobody and dist-straggler > (herd-threshold * 0.6)) and (rank = 0) [
      set is-collecting? true
      set color blue
      let sx [xcor] of straggler
      let sy [ycor] of straggler
      let v-len distancexy sx sy
      if v-len = 0 [ set v-len 0.001 ]
      let vec-x (sx - cx) / v-len
      let vec-y (sy - cy) / v-len
      set target-x sx + (vec-x * 4)
      set target-y sy + (vec-y * 4)
    ] [
      set color orange
      let focus-x cx
      let focus-y cy
      let gx item 0 current-goal
      let gy item 1 current-goal
      
      if scenario != "Open Field" [
         let dist-to-goal sqrt ((gx - cx)^ 2 + (gy - cy)^ 2)
         if dist-to-goal > 0 [
           set focus-x cx + ((gx - cx) / dist-to-goal) * 3.0
           set focus-y cy + ((gy - cy) / dist-to-goal) * 3.0
         ]
      ]

      let max-spread 0
      if any? sheep [ set max-spread max [distancexy cx cy] of sheep ]
      let orbit-dist max (list 10 (max-spread + 5))
      
      let angle-step 360 / total-dogs
      let base-angle (rank * angle-step)
      let patrol-offset (ticks * 2.0) 
      let my-angle (base-angle + patrol-offset)
      
      set target-x focus-x + (sin my-angle * orbit-dist)
      set target-y focus-y + (cos my-angle * orbit-dist)
    ]
  ]
  
  ;;;;;;;;;;;;;;;; Prevent dog from running directly through the herd center ;;;;;;;
  let dist-herd distancexy cx cy
  let dist-targ distancexy target-x target-y
    
  ;; Trigger conditions:
  ;; 1. Herd center is closer than the target (herd is in the middle)
  ;; 2. Dog is close to herd center (risk of splitting, buffer is radius + 5)
  if (dist-herd < dist-targ) and (dist-herd < herd-radius + 5) and (dist-herd > 0) [
     let ang-herd towardsxy cx cy
     let ang-targ towardsxy target-x target-y
       
     ;; 3. Angle check: If target angle and herd center angle are similar (< 60 deg), path is blocked
     if abs (subtract-headings ang-herd ang-targ) < 60 [
        ;; Calculate detour direction: Go left or right?
        let detour-turn 90
        if subtract-headings ang-targ ang-herd < 0 [ set detour-turn -90 ]
          
        ;; Modify target: Set to a tangent point on the herd edge (radius + 4 safety distance)
        ;; This makes the dog arc around the herd rather than through it
        set target-x cx + (sin (ang-herd + detour-turn) * (herd-radius + 4))
        set target-y cy + (cos (ang-herd + detour-turn) * (herd-radius + 4))
     ]
  ]

  move-dog-smart target-x target-y cx cy is-collecting?
end

to move-dog-smart [tx ty cx cy collecting?]
  let safe-pos sanitize-target tx ty cx cy
  set tx item 0 safe-pos
  set ty item 1 safe-pos
  
  let move-speed (max-speed / 5.0) * (dog-speed / 2.0)
  let dist-to-target distancexy tx ty
  
  ;; Iterative braking (avoid overshoot which leads to dog jumping way too far leading to bad performance (mostly when dog was super fast)
  let desired-speed min (list move-speed dist-to-target)
  
  if desired-speed > 0.1 [
    facexy tx ty
    let safe-speed 0
    let test-speed desired-speed
    let found-path? false
    
    while [test-speed > 0.1 and not found-path?] [
       let p-check patch-ahead (test-speed + 0.5)
       ifelse (p-check != nobody and [pcolor] of p-check != black) [
         set safe-speed test-speed
         set found-path? true
       ] [
         set test-speed test-speed * 0.5
       ]
    ]
    
    ifelse found-path? [
       fd safe-speed
    ] [
       facexy cx cy
       if can-move? move-speed [ fd move-speed ]
    ]
  ]
  
  ;; Orbiting adjustment (face inward)
  if not collecting? and distancexy cx cy < (herd-radius + 2) [
     set heading (towardsxy cx cy) + 180 
     if can-move? move-speed [ fd move-speed ] 
  ]
end


to-report sanitize-target [tx ty cx cy]
  ;; 1. Clamp to world boundaries
  if tx > max-pxcor [ set tx max-pxcor ] if tx < min-pxcor [ set tx min-pxcor ]
  if ty > max-pycor [ set ty max-pycor ] if ty < min-pycor [ set ty min-pycor ]

  ;; 2. Move towards center if inside a black wall
  let attempts 0
  
  ;; check for walls iteratively (black colored walls)
  while [ [pcolor] of patch tx ty = black and attempts < 100 ] [
    let dist-rem sqrt ((cx - tx) ^ 2 + (cy - ty) ^ 2)
    if dist-rem < 2 [ report (list cx cy) ] ;; Emergency exit to center
    
    let vx (cx - tx) / dist-rem
    let vy (cy - ty) / dist-rem
    
    set tx (tx + vx * 2) ;; Step closer to herd
    set ty (ty + vy * 2)
    set attempts attempts + 1
  ]
  
  report (list tx ty)
end

to calc-herd-metrics
  ifelse count sheep > 0 [
    let mx mean [xcor] of sheep
    let my mean [ycor] of sheep
    set herd-center (list mx my)
    let max-dist 0
    ask sheep [
      let d distancexy mx my
      if d > max-dist [ set max-dist d ]
    ]
    set herd-radius max-dist
  ] [
    set herd-center [0 0] set herd-radius 0
  ]
end

;; =============================================================================
;; SCENARIO CONTROL
;; =============================================================================

to draw-scenario
  ask patches [ set pcolor green + 2 ]
  
  ;; World Borders
  ask patches with [abs pxcor >= max-pxcor - 1 or abs pycor >= max-pycor - 1] [ set pcolor black ]
  
  if scenario = "Enclosure" [
    ;; 12x12 target area
    ask patches with [abs pxcor < 12 and abs pycor < 12] [ set pcolor brown ]
    ;; Walls
    ask patches with [(abs pxcor = 12 and abs pycor <= 12) or (abs pycor = 12 and abs pxcor <= 12)] [ set pcolor black ]
    ;; Gate
    ask patches with [pxcor = 12 and abs pycor < 5] [ set pcolor brown ]
    set current-goal [15 0]
  ]
  
  if scenario = "Path" [
    ask patches with [abs pycor < 6] [ set pcolor brown + 2 ]
    set waypoints (list (list (min-pxcor + 6) 0) (list 0 0) (list (max-pxcor - 6) 0))
    set waypoint-idx 0 
    set current-goal item 0 waypoints
  ]
  
  if scenario = "Target" [
    ask patches with [abs pxcor <= 6 and abs pycor <= 6] [ set pcolor brown ]
    set current-goal [0 0]
  ]
end

to update-scenario-goals
  let cx item 0 herd-center
  let cy item 1 herd-center
  
  if scenario = "Path" [
    let target item waypoint-idx waypoints
    let d sqrt ((cx - item 0 target) ^ 2 + (cy - item 1 target) ^ 2)
    if d < 8 and waypoint-idx < (length waypoints - 1) [
      set waypoint-idx waypoint-idx + 1 
      set current-goal item waypoint-idx waypoints
    ]
  ]
  
  if scenario = "Enclosure" [
    ifelse (abs cx < 12 and abs cy < 12) 
      [ set current-goal [0 0] ] 
      [ set current-goal [15 0] ]
  ]
end



; METRICS -----------------------------------
to update-metrics
  let n count sheep with [abs xcor <= 6 and abs ycor <= 6]
  set current-pct-in-target (n / num-sheep) * 100
  if tick-50pct = -1 and current-pct-in-target >= 50 [ set tick-50pct ticks ]
  if tick-80pct = -1 and current-pct-in-target >= 80 [ set tick-80pct ticks ]
  if tick-95pct = -1 and current-pct-in-target >= 95 [ set tick-95pct ticks ]
  if tick-100pct = -1 and current-pct-in-target >= 100 [ set tick-100pct ticks ]
  
  if ticks >= 2000 [ 
    set score-accumulator (score-accumulator + n) 
    set valid-ticks (valid-ticks + 1) 
  ]
end

to-report get-success-percentage
  if valid-ticks = 0 [ report 0 ]
  report (score-accumulator / valid-ticks / num-sheep) * 100
end





to-report get-tick-50pct
  report tick-50pct
end

to-report get-tick-80pct
  report tick-80pct
end

to-report get-tick-95pct
  report tick-95pct
end

to-report get-tick-100pct
  report tick-100pct
end]]></code>
